{
  "name": "@salmanbao/jackal.nodejs",
  "version": "1.0.0",
  "description": "Javascript Node.js library for interacting with the Jackal Chain",
  "keywords": [
    "jackal",
    "canine",
    "nodejs"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./tsc && npm run tidy",
    "tidy": "rollup -c && rm -R dist/tsc",
    "packit": "npm run build && npm pack --pack-destination packs",
    "gen-docs": "npx typedoc"
  },
  "contributors": [
    {
      "name": "Jackal Labs",
      "email": "devadmin@jackallabs.io",
      "url": "https://jackallabs.io"
    },
    {
      "name": "Erin Rivas",
      "email": "erin@jackallabs.io"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salmanbao/jackal.nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/salmanbao/jackal.nodejs.git/issues"
  },
  "homepage": "https://github.com/salmanbao/jackal.nodejs.git/#README",
  "dependencies": {
    "@cosmjs/launchpad": "^0.27.1",
    "@cosmjs/proto-signing": "^0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "eciesjs": "^0.4.2",
    "@jackallabs/jackal.nodejs-protos": "^1.0.2"
  },
  "devDependencies": {
    "@btmills/prettier": "^2.8.8",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.0",
    "typedoc": "^0.24.8",
    "typescript": "^4.9.4"
  }
}
